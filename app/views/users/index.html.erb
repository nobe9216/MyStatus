<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>年齢</th>
      <th>身長</th>
      <th>体重（開始時）</th>
      <th>体重（目標）</th>
      <th>性別</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.nickname %></td>
        <td><%= user.age %></td>
        <td><%= user.height %></td>
        <td><%= user.start_weight %></td>
        <td><%= user.goal_weight %></td>
        <td><%= user.sex %></td>
      </tr>
    <% end %>
  </tbody>
</table>